class Student 
  attr_reader :name, :email, :contact
  attr_writer :name, :email, :contact
  def initialize(name, email, contact)
    @name = name
    @email = email
    @contact = contact
  end
end

s1 = Student.new('Tufail baba', 'tufailbabacoc@gmail.com', '1235678')
puts s1.name
puts s1.email
puts s1.contact
