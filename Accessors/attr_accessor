class Student 
  attr_accessor :name, :email, :contact
  
  def initialize(name, email, contact)
    @name = name
    @email = email
    @contact = contact
  end
end

s1 = Student.new('Tufail baba', 'tufailbabacoc@gmail.com', '1235678')
s1.name='Tufi'
s1.email='tufi@gmail.com'
s1.contact= '12344567'
puts s1.name
puts s1.email
puts s1.contact
