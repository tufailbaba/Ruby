class Student 
  def initialize(name, email, contact)
    @name = name
    @email = email
    @contact = contact
  end

  # getter methods
  def name
    @name
  end

  def email
    @email
  end

  def contact
    @contact
  end

  # setter methods
  def name=(name)
    @name = name
  end

  def email=(email)
    @email = email
  end

  def contact=(contact)
    @contact = contact
  end
end

s1 = Student.new('Tufail baba', 'tufailbabacoc@gmail.com', '1235678')
puts s1.name
puts s1.email
puts s1.contact
